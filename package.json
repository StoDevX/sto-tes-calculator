{
  "name": "sto-tes-calculator",
  "private": true,
  "version": "0.0.0",
  "description": "A time calculator for St. Olaf's TES",
  "main": "src/index.js",
  "config": {
    "ify": "--debug -t [ babelify --experimental ] -e src/index.js",
    "less": "--source-map-map-inline --strict-math=on  --autoprefix src/style.less build/style.css"
  },
  "scripts": {
    "test": "jest",
    "env": "env",
    "prebuild": "mkdir -p build && npm run copy",
    "copy": "cp src/index.html build",
    "js": "browserify $npm_package_config_ify | exorcist build/bundle.js.map > build/bundle.js",
    "css": "lessc $npm_package_config_less",
    "build": "npm run js & npm run css; wait",
    "prewatch": "mkdir -p build && npm run copy",
    "watchify": "watchify --verbose $npm_package_config_ify -o build/bundle.js",
    "lesswatch": "nodemon --exec 'npm run css' --ext less --watch src",
    "watch": "npm run watchify & npm run lesswatch; wait",
    "bs": "browser-sync start --server build --files 'build/*.css, build/*.js, build/*.html'",
    "serve": "npm run bs & npm run watch; wait"
  },
  "repository": "hawkrives/sto-tes-calculator",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hawkrives/sto-tes-calculator/issues"
  },
  "homepage": "https://github.com/hawkrives/sto-tes-calculator",
  "devDependencies": {
    "babelify": "^5.0.3",
    "browser-sync": "^2.1.1",
    "browserify": "^8.1.3",
    "jest": "^0.1.37",
    "less": "^2.4.0",
    "less-plugin-autoprefix": "^1.3.0",
    "nodemon": "^1.3.7",
    "watchify": "^2.3.0"
  },
  "dependencies": {
    "lodash": "^3.2.0",
    "moment": "^2.9.0",
    "react": "^0.12.2",
    "react-router": "^0.12.0",
    "reflux": "^0.2.5",
    "round": "^1.0.1",
    "whatwg-fetch": "^0.7.0"
  }
}
